<% if (messages && messages.length) { %>
    <div class="row">
        <div class="col-md-4 offset-md-4 alert alert-success alert-dismissible fade show" role="alert">
            <%= messages %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" id="liveAlertBtn"></button>
        </div>    
    </div>
<% } %>